{
    "name": "quick-bite",
    "version": "1.0.0",
    "workspaces": [
        "packages/*",
        "apps/*"
    ],
    "scripts": {
        "build:client": "npm --prefix apps/client run build",
        "build:restaurant": "npm --prefix apps/restaurant run build",
        "build": "run-p build:*",
        "preview:client": "npm --prefix apps/client run preview",
        "preview:restaurant": "npm --prefix apps/restaurant run preview",
        "preview": "run-p preview:*",
        "dev:client": "npm --prefix apps/client run dev",
        "dev:restaurant": "npm --prefix apps/restaurant run dev",
        "dev": "run-p dev:*"
    },
    "devDependencies": {
        "npm-run-all": "^4.1.5"
    },
    "dependencies": {
        "@dnd-kit/core": "^6.1.0",
        "@dnd-kit/sortable": "^8.0.0",
        "@dnd-kit/utilities": "^3.2.2",
        "@fortawesome/fontawesome-free": "^6.5.2",
        "date-fns": "^3.6.0",
        "react-beautiful-dnd": "^13.1.1",
        "react-dnd-html5-backend": "^16.0.1",
        "react-dom": "^18.2.0",
        "react-draggable": "^4.4.6",
        "react-router-dom": "^6.22.3",
        "uuid": "^9.0.1"
    }
}
